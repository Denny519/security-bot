{
  "name": "security-bot",
  "version": "2.2.0",
  "description": "Advanced Discord Security and Moderation Bot",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "deploy": "node deploy-commands.js",
    "test": "jest",
    "test-bot": "node test-bot.js",
    "test-deploy": "node test-deploy.js",
